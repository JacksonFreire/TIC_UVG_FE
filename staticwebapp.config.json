{
    "routes": [
      {
        "route": "/*",
        "serve": "/index.html",
        "statusCode": 200
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": [
        "/images/*",
        "/css/*",
        "/js/*",
        "/favicon.ico"
      ]
    },
    "mimeTypes": {
      ".json": "application/json",
      ".ico": "image/x-icon",
      ".css": "text/css",
      ".js": "application/javascript"
    }
  }
  